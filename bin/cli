#!/usr/bin/env php
<?php

error_reporting(-1);

defined('ROOT_PATH') || define('ROOT_PATH', dirname(__DIR__));

include ROOT_PATH . '/bootstrap/functions.php';
include ROOT_PATH . '/bootstrap/di.php';

use Phwoolcon\Cli;

$_SERVER['PHWOOLCON_MIGRATION_PATH'] = ROOT_PATH . '/bin/migrations';
$app = Cli::register($di);
$app->run();
